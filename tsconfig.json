{
  "compilerOptions": {
    "target": "es2016",
    "downlevelIteration": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "lib": ["es2016", "esnext.asynciterable", "dom"],
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "noUnusedLocals": true,
    "declaration": true,
    "declarationMap": true,
    "importHelpers": true,
    "resolveJsonModule": true,
    "sourceMap": true
  },
  "include": ["src"]
}
